USE IBM_ANALYTICS;
GO

IF SCHEMA_ID('bi') IS NULL
    EXEC ('CREATE SCHEMA bi');
GO


CREATE OR ALTER VIEW bi.DEPARTMENT AS
SELECT * FROM dbo.DEPARTMENT;
GO

CREATE OR ALTER VIEW bi.JOB_ROLE AS
SELECT * FROM dbo.JOB_ROLE;
GO

CREATE OR ALTER VIEW bi.EDUCATION AS
SELECT * FROM dbo.EDUCATION;
GO

CREATE OR ALTER VIEW bi.EMPLOYEE AS
SELECT * FROM dbo.EMPLOYEE;
GO

CREATE OR ALTER VIEW bi.EMPLOYEE_JOB AS
SELECT * FROM dbo.EMPLOYEE_JOB;
GO

CREATE OR ALTER VIEW bi.EMPLOYEE_EDUCATION AS
SELECT * FROM dbo.EMPLOYEE_EDUCATION;
GO

CREATE OR ALTER VIEW bi.SALARY AS
SELECT * FROM dbo.SALARY;
GO

CREATE OR ALTER VIEW bi.PERFORMANCE AS
SELECT * FROM dbo.PERFORMANCE;
GO

CREATE OR ALTER VIEW bi.SATISFACTION AS
SELECT * FROM dbo.SATISFACTION;
GO
